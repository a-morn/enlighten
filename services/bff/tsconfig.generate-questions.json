{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "lib": [
      "es6"
    ],
    "allowJs": true,
    "strict": true,
    "noImplicitAny": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "rootDir": "question-generation-scripts",
    "baseUrl": "./",
    "paths": {
      "*": [
        "./node_modules/@types/*",
        "*"
      ]
    },
  },
  "include": [
    "question-generation-scripts/**/*"
  ]
}